<h1>Questions</h1>
<table>
  <tr>
    <td>Title</td>
    <td>Question</td>
  </tr>
  <tr>
    <% @questions.each do |question|  %>
      <td><%= link_to question.title, question  %></td>
      <td><%= question.content  %></td>
  </tr>
    <% end %>
</table>

<%= link_to "Create question", new_question_path %>
  




    
